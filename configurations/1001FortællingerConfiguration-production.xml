			<chc:FormatId>42</chc:FormatId>
			<chc:Destination name="default">
				<chc:baseURL>http://www.kulturarv.dk/1001fortaellinger/</chc:baseURL>
				<chc:id>82</chc:id>
			</chc:Destination>
		</chc:FileProcessor>
		<chc:FileProcessor className="LowresImageFileProcessor"
